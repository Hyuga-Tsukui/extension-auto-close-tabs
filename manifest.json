{
	"manifest_version": 3,
	"name": "Auto Close Tabs",
	"version": "1.0",
	"permissions": ["tabs", "storage", "alarms", "tabGroups"],
	"background": {
		"service_worker": "dist/service_worker.js"
	},
	"action": {
		"default_title": "Set auto-close delay default 12h",
		"default_popup": "popup.html",
		"default_icon": "icons/icon16.png"
	},
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}
